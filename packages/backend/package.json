{
  "name": "@fourside/backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "yarn clean",
    "start:dev": "nodemon --config nodemon.json",
    "start:prod": "NODE_ENV=production node dist/app/bootstrap.js",
    "start:swagger": "IS_SWAGGER_PROCESS=true nodemon --watch ./src --ext ts --exec 'ts-node -r tsconfig-paths/register ./src/swagger/bootstrap.ts'",
    "start:swagger:prod": "node dist/swagger/swagger-ui-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rimraf dist",
    "format:swagger": "prettier --write swagger.json",
    "seed:asset": "ts-node src/asset-seed-data/asset-seed-data.seed.ts",
    "seed:asset:pdf-merge": "ts-node src/asset-seed-pdf-merger/asset-seed-pdf-merger.seed.ts",
    "seed:grade": "ts-node src/grade-seed-data/grade-seed-data.seed.ts",
    "seed:grade:csv": "ts-node src/grade-seed-data/grade-seed-data.seed-csv.ts",
    "typeorm:migration:generate": "NODE_ENV=development ts-node node_modules/.bin/typeorm -f src/type-orm.option.ts migration:generate",
    "migration:generate": "yarn build && yarn typeorm:migration:generate",
    "postmigration:generate": "prettier --write src",
    "migration:revert": "NODE_ENV=development ts-node node_modules/.bin/typeorm -f src/type-orm.option.ts migration:revert",
    "migration:run": "NODE_ENV=development ts-node node_modules/.bin/typeorm -f src/type-orm.option.ts migration:run",
    "migration:run:prod": "NODE_ENV=production ts-node node_modules/.bin/typeorm -f src/type-orm.option.ts migration:run"
  },
  "dependencies": {
    "@anchan828/nest-sendgrid": "0.4.22",
    "@google-cloud/logging-winston": "4.0.5",
    "@google-cloud/pubsub": "2.11.0",
    "@google-cloud/storage": "5.8.5",
    "@nestjs/common": "7.6.17",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.17",
    "@nestjs/platform-express": "7.6.17",
    "@nestjs/swagger": "4.8.0",
    "@nestjs/typeorm": "7.1.5",
    "@fourside/interface": "0.0.1",
    "@fourside/nestjs-firebase": "1.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "csv": "5.5.0",
    "csvtojson": "2.0.10",
    "csv-parse": "4.15.4",
    "date-fns": "2.22.1",
    "date-fns-tz": "1.1.4",
    "dotenv": "8.6.0",
    "dropbox": "9.7.0",
    "envalid": "6.0.2",
    "express": "4.17.1",
    "firebase-admin": "9.7.0",
    "generate-password": "1.6.0",
    "google-auth-library": "7.1.2",
    "googleapis": "67.1.1",
    "mysql": "2.18.1",
    "pdf-merger-js": "3.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.36",
    "typeorm-transactional-cls-hooked": "0.1.20",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "7.6.17",
    "@types/express": "4.17.12",
    "@types/generate-password": "1.5.0",
    "@types/node": "13.13.51",
    "@types/rimraf": "3.0.1",
    "@types/uuid": "8.3.1",
    "eslint-plugin-prettier": "3.4.0",
    "iconv-lite": "0.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
