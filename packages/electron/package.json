{
  "name": "@fourside/electron",
  "version": "0.0.1",
  "main": "dist/main.js",
  "license": "UNLICENSED",
  "scripts": {
    "rebuild:mac": "electron-rebuild",
    "rebuild:win": "./node_modules/.bin/electron-rebuild.cmd",
    "webpack": "webpack --config ./configs/webpack/webpack.config.ts",
    "build": "yarn build:prod",
    "build:local": "cross-env NODE_ENV=development DEPLOY_ENV=local yarn webpack",
    "build:dev": "cross-env NODE_ENV=development DEPLOY_ENV=dev yarn webpack",
    "build:test": "cross-env NODE_ENV=production DEPLOY_ENV=test yarn webpack",
    "build:demo": "cross-env NODE_ENV=production DEPLOY_ENV=demo yarn webpack",
    "build:prod": "cross-env NODE_ENV=production DEPLOY_ENV=prod yarn webpack",
    "watch": "yarn build:local && yarn start:server",
    "start:prod": "yarn build:prod && electron ./dist/main.js",
    "start:server": "cross-env NODE_ENV=development DEPLOY_ENV=local webpack-dev-server --config ./configs/webpack/webpack.config.server.ts",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint --fix -c ./.eslintrc.json './src/**/*.{ts,tsx}'",
    "test": "jest",
    "clean": "rimraf dist",
    "clean:app": "rimraf Fourside"
  },
  "engine": {
    "node": "14.x",
    "yarn": "^1.16.0"
  },
  "dependencies": {
    "@babel/preset-env": "7.7.1",
    "@fourside/interface": "0.0.1",
    "@material-ui/core": "4.9.7",
    "babel-plugin-import": "1.12.2",
    "connected-react-router": "6.7.0",
    "css-loader": "3.2.0",
    "electron-log": "4.3.5",
    "electron-store": "5.2.0",
    "electron-updater": "4.3.5",
    "formik": "2.2.9",
    "history": "4.10.1",
    "query-string": "6.11.1",
    "react": "16.13.1",
    "react-countdown": "2.2.1",
    "react-countup": "4.3.3",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "style-loader": "1.0.0",
    "use-image": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/register": "7.14.5",
    "@babel/runtime": "^7.14.8",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-backgrounds": "5.3.18",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.18",
    "@types/history": "4.7.5",
    "@types/jest": "24.9.1",
    "@types/react": "16.9.33",
    "@types/react-dom": "16.9.14",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-redux": "7.1.18",
    "@types/react-router": "5.1.16",
    "@types/redux-mock-store": "1.0.2",
    "@types/webpack-merge": "5.0.0",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.2",
    "electron": "11.4.2",
    "electron-builder": "22.10.5",
    "electron-devtools-installer": "3.2.0",
    "electron-notarize": "1.0.0",
    "electron-rebuild": "2.3.5",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "5.3.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "react-hot-loader": "4.12.20",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "source-map-loader": "0.2.4",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.2",
    "webpack": "5.49.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "4.0.0-rc.0",
    "webpack-merge": "5.8.0"
