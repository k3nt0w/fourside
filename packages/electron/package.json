{
  "name": "@fourside/electron",
  "version": "0.0.1",
  "main": "dist/main.js",
  "license": "UNLICENSED",
  "scripts": {
    "rebuild:mac": "electron-rebuild",
    "rebuild:win": "./node_modules/.bin/electron-rebuild.cmd",
    "build": "yarn build:dev",
    "webpack": "webpack --config ./configs/webpack/webpack.config.ts --progress",
    "build:local": "cross-env NODE_ENV=development DEPLOY_ENV=local yarn webpack",
    "build:dev": "cross-env NODE_ENV=development DEPLOY_ENV=dev yarn webpack",
    "build:test": "cross-env NODE_ENV=production DEPLOY_ENV=test yarn webpack",
    "build:demo": "cross-env NODE_ENV=production DEPLOY_ENV=demo yarn webpack",
    "build:prod": "cross-env NODE_ENV=production DEPLOY_ENV=prod yarn webpack",
    "start": "electron ./dist/main.js",
    "watch": "yarn clean && yarn build:local && cross-env NODE_ENV=development DEPLOY_ENV=local webpack-cli serve --config ./configs/webpack/webpack.config.server.ts",
    "lint": "eslint --fix -c ./.eslintrc.json './src/**/*.{ts,tsx}'",
    "test": "jest",
    "clean": "rimraf dist",
    "clean:app": "rimraf Fourside",
    "release:mac": "cross-env DEPLOY_ELECTRON=true yarn electron-builder --mac --config electron-builder.js --publish always",
    "release:win": "cross-env DEPLOY_ELECTRON=true yarn electron-builder --win --config electron-builder.js --publish always",
    "package:all:test": "yarn build:test && electron-builder -mw --config electron-builder.js --publish always",
    "package:mac:dev:local": "cross-env PACKAGE=true yarn build:dev && cross-env DEPLOY_ENV=local yarn electron-builder --mac --config electron-builder.js",
    "package:mac:test:local": "cross-env PACKAGE=true yarn build:test && cross-env DEPLOY_ENV=local yarn electron-builder --mac --config electron-builder.js",
    "package:mac:prod:local": "cross-env PACKAGE=true yarn build:prod && cross-env DEPLOY_ENV=local yarn electron-builder --mac --config electron-builder.js",
    "package:win:dev:local": "cross-env PACKAGE=true yarn build:dev && cross-env DEPLOY_ENV=local yarn electron-builder --win --x64 --config electron-builder.js",
    "package:win:test:local": "cross-env PACKAGE=true yarn build:test && cross-env DEPLOY_ENV=local yarn electron-builder --win --x64 --config electron-builder.js",
    "package:win:prod:local": "cross-env PACKAGE=true yarn build:prod && cross-env DEPLOY_ENV=local yarn electron-builder --win --x64 --config electron-builder.js",
    "storybook": "NODE_OPTIONS=--max_old_space_size=4096 start-storybook --debug-webpack -p 9001"
  },
  "engine": {
    "node": "14.x",
    "yarn": "^1.16.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/register": "7.15.3",
    "@storybook/addon-actions": "6.3.7",
    "@storybook/addon-backgrounds": "6.3.7",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.3.7",
    "@storybook/addons": "6.3.7",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "6.3.7",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/history": "4.7.9",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-redux": "7.1.18",
    "@types/react-router": "5.1.16",
    "@types/redux-mock-store": "1.0.3",
    "@types/webpack-env": "1.16.2",
    "@types/webpack-merge": "4.1.5",
    "@webpack-cli/serve": "1.5.2",
    "babel-loader": "8.2.2",
    "copy-webpack-plugin": "9.0.1",
    "electron": "13.2.1",
    "electron-builder": "22.11.7",
    "electron-devtools-installer": "3.2.0",
    "electron-notarize": "1.1.0",
    "electron-rebuild": "2.3.5",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.2",
    "node-loader": "2.0.0",
    "react-hot-loader": "4.13.0",
    "redux-devtools-extension": "2.13.9",
    "redux-mock-store": "1.5.4",
    "source-map-loader": "1.1.3",
    "storybook-addon-material-ui": "0.9.0-alpha.24",
    "ts-loader": "8.3.0",
    "webpack": "5.51.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "babel-plugin-import": "1.12.2",
    "connected-react-router": "6.9.1",
    "cross-env": "7.0.3",
    "css-loader": "5.0.0",
    "electron-log": "4.4.1",
    "electron-store": "8.0.0",
    "electron-updater": "4.3.9",
    "formik": "2.2.9",
    "formik-material-ui": "3.0.1",
    "history": "4.10.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "rc-notification": "4.5.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "redux": "4.1.1",
    "redux-thunk": "2.3.0",
    "reflect-metadata": "0.1.13",
    "reselect": "4.0.0",
    "semver": "7.3.5",
    "style-loader": "3.2.1"
  }
}
